{
  "expo": {
    "name": "Offline-First",
    "slug": "Offline-First",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "offlinefirst",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nachonacho.Offline-First",
      "appleTeamId": "SGNPJU43HD",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to add images to tasks.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos for tasks.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to add location information to tasks."
      }
    },
    "android": {
      "package": "com.nachonacho.OfflineFirst",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "85ff9d28-8b85-49f4-b85b-6932f0634f2b"
      }
    }
  }
}
